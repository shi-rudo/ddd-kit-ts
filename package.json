{
	"name": "@shirudo/ddd-kit",
	"version": "0.9.6",
	"description": "Composable TypeScript toolkit for tactical DDD",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/shi-rudo/ddd-kit-ts.git"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./result": {
			"import": "./dist/result.js",
			"types": "./dist/result.d.ts"
		},
		"./utils": {
			"import": "./dist/utils.js",
			"types": "./dist/utils.d.ts"
		},
		"./utils/array": {
			"import": "./dist/utils-array.js",
			"types": "./dist/utils-array.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"test": "vitest",
		"clean": "rm -rf dist",
		"lint": "biome lint .",
		"format": "pnpm exec biome format --write . && pnpm exec biome lint --write . && pnpm exec biome check --write .",
		"typecheck": "tsc --noEmit"
	},
	"keywords": [
		"ddd",
		"domain",
		"typescript",
		"toolkit"
	],
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.4",
		"tsup": "^8.5.0",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4"
	},
	"private": false,
	"author": "Shirudo",
	"contributors": [
		"Shirudo"
	],
	"homepage": "https://github.com/shi-rudo/ddd-kit-ts",
	"bugs": {
		"url": "https://github.com/shi-rudo/ddd-kit-ts/issues"
	}
}
